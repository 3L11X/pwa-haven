{
  "name": "pwa-haven",
  "version": "1.0.0",
  "private": true,
  "standard": {
    "ignore": [
      "/build/*",
      "/lib/subtitles-octopus-worker.js"
    ],
    "env": "browser"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv --no-clear",
    "build-manga-reader": "rollup -c manga-reader.rollup.js",
    "dev-manga-reader": "rollup -w -c manga-reader.rollup.js",
    "build-img-viewer": "rollup -c img-viewer.rollup.js",
    "dev-img-viewer": "rollup -w -c img-viewer.rollup.js",
    "build-video-player": "cross-env NODE_ENV=production webpack --config ./video-player.webpack.js",
    "dev-video-player": "webpack serve --config ./video-player.webpack.js",
    "build-audio-player": "cross-env NODE_ENV=production webpack --config ./audio-player.webpack.js",
    "dev-audio-player": "webpack serve --config ./audio-player.webpack.js",
    "build-torrent-client": "cross-env NODE_ENV=production webpack --config ./torrent-client.webpack.js",
    "dev-torrent-client": "webpack serve --config ./torrent-client.webpack.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "mini-css-extract-plugin": "^2.5.2",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "pako": "^2.0.4",
    "process-fast": "^1.0.0",
    "rollup": "^2.65.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "standard": "^16.0.4",
    "svelte": "^3.46.2",
    "svelte-loader": "^3.1.2",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "end-of-stream": "^1.4.4",
    "fast-blob-stream": "github:ThaUnknown/fast-blob-stream",
    "hybrid-chunk-store": "github:ThaUnknown/hybrid-chunk-store",
    "idb-keyval": "^6.1.0",
    "matroska-subtitles": "^3.3.2",
    "music-metadata-browser": "^2.5.3",
    "range-parser": "^1.2.1",
    "sirv-cli": "^1.0.0",
    "speedometer": "^1.1.0",
    "streamx": "^2.12.0",
    "unzipit": "^1.4.0",
    "webtorrent": "^1.7.1"
  }
}
